{
  "training": {
    "optimizer": "sgd",
    "learning_rate": 1.0,
    "max_norm": 3.0,
    "epochs": 70,
    "batches_per_report": 1,
    "batches_per_sampling": 500,
    "random_seed": 1
  },
  "data": {
    "src": "test_data/train.ja",
    "tgt": "test_data/train.en",
    "src_test": "test_data/test.ja",
    "tgt_test": "test_data/test.en",
    "src_vocab": "test_data/vocab.12000.ja",
    "tgt_vocab": "test_data/vocab.12000.en",
    "share_vocab": false,
    "batch_size": 20,
    "max_len": 50,
    "working_dir": "sample_run"
  },
    "model": {
        "emb_dim": 32,
        "attention": false,
        "encoder": "lstm",
        "src_hidden_dim": 64,
        "src_layers": 1,
        "bidirectional": true,
        "tgt_hidden_dim": 64,
        "tgt_layers": 1,
        "decode": "greedy",
        "dropout": 0.2
    }
}

